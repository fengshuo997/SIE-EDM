
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix iasis: <http://project-iasis.eu/vocab/> .
@base <http://project-iasis.eu/> .

<TripleMap1>
  a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "./example/rdfizer/example/files/btc_tower.csv";
    rml:referenceFormulation ql:CSV
  ];
  rr:subjectMap [
    rr:template "http://project-iasis.eu/{ENERGY_SOURCE}";
    rr:class iasis:ENERGY_SOURCE
  ]; 
  rr:predicateObjectMap [ 
    rr:predicate iasis:has_measure; 
    rr:objectMap [
      rr:parentTriplesMap <TripleMap2>;
      rr:joinCondition [
        rr:child "TIMESTAMP";
        rr:parent "TIMESTAMP";
      ];
    ];
  ].

